{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/recommended",
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "comma-dangle": [
      "error",
      "always"
    ]
  }
}